{
    "environment": {
        "name": "default-mask"
    },
    "transformation": {
        "name": "train_window_predict_mask",
        "options": {
            "size": 25,
            "step": 1,
            "shuffle": true,
            "save_audio": false
        }
    },
    "training": {
        "model": {
            "name": "mask",
            "options": {
                "alpha": 0.3,
                "dropout_rate": 0.5,
                "filter_factor": 1
            }
        },
        "batch_size": 8,
        "loss_function": "L11_loss",
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        }
    }
}