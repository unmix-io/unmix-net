{
    "environment": {
        "name": "default",
        "data_folder": "",
        "weights": {
            "file": "/weights/weights.h5",
            "load": "str2bool(True)"
        },
        "log_folder": "/logs",
        "tensorboard_folder": "/TensorBoard",
        "summary_line_length": "int(150)"
    },
    "training": {
        "epochs": 10,
        "loss_function": "mean_squared_error",
        "optimizer": "adam",
        "metrics": ["mean_pred"],
        "choppers": [
            {
                "direction": "horizontal",
                "mode": "split",
                "size": "int(64)"
            }
        ],
        "model": {
            "name": "LeakyReLU",
            "alpha1": "float(0.1)",
            "alpha2": "float(0.01)",
            "rate": "float(0.1)"
        }
    }
}